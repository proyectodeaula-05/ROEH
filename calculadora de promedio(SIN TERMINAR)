def calcular_promedio(datos):
    total = sum(datos)
    promedio = total / len(datos)
    return promedio

def main():
    datos = []
    for i in range(6):
        dato = float(input("Ingrese el dato {}: ".format(i+1)))
        datos.append(dato)

    promedio = calcular_promedio(datos)
    print("El promedio de los datos es:", promedio)

if __name__ == "__main__":
    main()